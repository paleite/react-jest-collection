{
  "name": "react-jest-collection",
  "version": "1.0.0",
  "private": true,
  "description": "Write your React integration tests using battle-tested test-examples",
  "keywords": [],
  "homepage": "https://github.com/paleite/react-jest-collection#readme",
  "bugs": {
    "url": "https://github.com/paleite/react-jest-collection/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleite/react-jest-collection.git"
  },
  "license": "MIT",
  "author": "Patrick Eriksson",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "postinstall": "yarn build",
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.19",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cspell": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-files": "^1.0.0",
    "eslint-plugin-diff": "^1.0.12",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "^14"
  }
}
